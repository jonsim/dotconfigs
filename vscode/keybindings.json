// Place your key bindings in this file to overwrite the defaults
[
    { "key": "ctrl+d",            "command": "editor.action.deleteLines",
                                     "when": "editorTextFocus && !editorReadonly" },
    { "key": "ctrl+g",            "command": "editor.action.nextMatchFindAction",
                                     "when": "editorFocus" },
    { "key": "ctrl+shift+g",      "command": "editor.action.previousMatchFindAction",
                                     "when": "editorFocus" },
    { "key": "ctrl+l",            "command": "workbench.action.gotoLine" },
    { "key": "f3",                "command": "editor.action.addSelectionToNextFindMatch",
                                     "when": "editorFocus" }
]